(function(zd){
    zd.os = {};
    var ua = navigator.userAgent.toLowerCase(), pf = navigator.platform.toLowerCase();
    zd.os.isandroid = ua.indexOf('android') > -1 &&  pf.indexOf('linux') > -1;
    zd.os.isiphone = ua.indexOf('iphone') > -1 &&  pf.indexOf('iphone') > -1;
    zd.os.iswechat = ua.indexOf('micromessenger') > -1;
    zd.os.isalipay = ua.indexOf('alipayclient') > -1;
    zd.os.iswebo = ua.indexOf('weibo') > -1;
    zd.os.isqq = ua.indexOf('qq') > -1 && ua.indexOf('mqqbrowser') == -1;
    zd.os.issocialapp = zd.os.iswechat || zd.os.iswebo || zd.os.isqq;
})(ttzd);