body {
    background: none repeat scroll 0 0 #eee;
    font-size: 12px;
    font-weight: normal;
    line-height: 1.25;
    color: #666;
}
th, td {
    text-align: center;
    vertical-align: top;
}
h1, h2, h3, h4, h5, h6 {
    font-size: 1em;
    font-weight: normal;
}
em,i{font-style: normal;}
font {
    font-size: 12px;
}
a {
    color:#69B3E2;
    text-decoration: none;
}
a.visited {
    color: #000000;
    text-decoration: none;
}
a:hover {
    text-decoration: none;
}
a.current:hover {
    cursor: default;
    text-decoration: none;
}
img {
    border: 0 none;
}
li {
    list-style: none outside none;
}
input, label, select, option, textarea, button, fieldset, legend {
    /*font-family: Arial;*/
    font-size: 1em;
}
input {
    font-size: 120%;
}

