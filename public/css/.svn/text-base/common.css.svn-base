footer nav a {display: inline-block;height: 40px;text-align: center;line-height: 45px;color: #000;vertical-align: top;font-size: 14px;}
.btm_ads{height: 36px;padding: 7px 19px 7px 10px;background-color: #fff;border-top:1px solid #dedede;}
.btm_p{color: #2c3e50;padding:0 6px 0 5px;line-height:18px; float: left;}
.btn_pink_small{display: block; width:56px;height:25px;background-color:#F05B72;color: #FFF;text-align: center;line-height:25px;text-decoration: none;border-radius:3px;-webkit-border-radius:3px;margin-top: 6px;}
.btm_index_bg{background-color: #000;opacity: 0.7; height: 50px;position: fixed;bottom: 0;width: 100%;}
.btm_index{position: fixed;bottom: 0;background: none;padding: 7px 5px 7px 5px;width: 97%;}
.btm_index .btm_p{color: #fff;}
.btm_index .top_close{margin:8px 0 0 6px;}

.icon_dot{background:url(../images/icon_dot.png) no-repeat center; background-size:8px 8px;width:8px; height:8px; margin-right: 3px; display: inline-block;}
.dot_more{position: absolute;right:7px;top:9px; border: solid 2px #fff;}
.padding10{padding: 10px;}
.icon_photo{background: url("../images/icon_photo.png") no-repeat;background-size: 20px 16px;width: 20px; height: 16px;display: inline-block;vertical-align: middle;margin-right: 5px;}
.icon_take_photo{background: url("../images/icon_take_photo.png") no-repeat;background-size: 20px 16px;width: 20px; height: 16px;display: inline-block;vertical-align: middle;margin-right: 5px;}

/*country mobile num*/ 
.country_num{position:absolute; top:11px; right:15px; width:86px; height:15px; line-height:15px; border-left:1px solid #959ea7;} 
.icon_country{ float:right; display:inline-block; width:20px; height:14px; background:url(../images/flag/China.jpg); background-size:20px 14px; margin-right:5px; vertical-align:-2px;border: 1px solid #eeeeee;}
.code{float:right;color: #323232;}
.b_arrow{ float:right; display:inline-block; background:url(../images/arrow.png) no-repeat; width:6px; height:4px; background-size:6px 4px; margin: 6px 0 0 5px;} 
/*select country*/ 
.first_letter{ height:22px; line-height:22px; background:#eee; padding-left:20px; color:#323232; font-size:12px;} 
.country_li{ height:50px; line-height:50px; border-bottom:1px solid #e0e0e0; font-size:14px; padding-right:20px;margin:0 10px} 
.country_li:last-child{ border:0;}
.country_l{ float:left; color:#323232;}
.country_r{ float:right; color:#666;}
.scroll_bar{ position:fixed; top:122px; right:5px; background:#f3f3f3; width:15px; border-radius:15px; -webkit-border-radius:15px; padding:8px 0;} 
.scroll_bar .c_star,.scroll_bar .red_star{ display:block; background:url(../images/c_star.png) no-repeat; background-size:8px 8px; height:10px; margin-left:4px; }
.scroll_bar .red_star {background: url(../images/red_star.png) no-repeat;background-size: 8px 8px;}
.scroll_bar i{font-size:12px; display:block; text-align:center; height:16px; line-height:16px;}
.choiced{background-color: #F05B72;color: #fff;} 
.active{color:red;}
/*首页引导至app弹层*/
a,button,input{-webkit-tap-highlight-color:rgba(255,0,0,0); -webkit-touch-callout:none;}
.download_mask{ background:rgba(0,0,0,0.5); position:fixed; top:0; left:0; width:100%; height:100%;z-index: 9999;}
.download_box{ position:absolute; top:50%; left:50%; width:80%;z-index: 999; -webkit-transform: translate(-50%,-50%); -moz-transform: translate(-50%,-50%); -ms-transform: translate(-50%,-50%); -o-transform: translate(-50%,-50%); transform: translate(-50%,-50%);}
.download_box img{ width:100%;}
.download_close{ position:absolute; top:0; right:3%; width:7%; height:7%;}
.download_btn{ position:absolute; bottom:3%; width:40%; height:11%; left:50%; transform:translateX(-50%);}

/*ledtoapp导航条*/
.imp_box{ position:fixed; top:0; left:0; width:100%; height:45px; background:#f9e0e3;z-index: 999;}
.imp_close{ display:block; position:absolute; top:0; left:0; width:18px; height:18px; background:url(../images/imp_close.jpg); background-size:18px 18px;}
.imp_h4{ color:#f05b72; font-weight:bold; padding:10px 0 0 20px;}
.imp_h4 img{ vertical-align:-9px; margin-right:6px;}
.gohome_btn{ position: absolute; top:9px; right:15px; display: block; width: 65px; height: 26px; line-height: 26px; text-align: center; background: #f05b72; color: #fff; font-weight: bold;}
/*ledtowechat导航条*/
/*led open in browser*/
.downapp_tbox{ position:relative; text-align:center;}
.guide_line{ display:block; position:absolute; width:46%; left:50%; top:5px; }
.tubiao{ margin:22% auto 4%;}
.downapp_tbox p{ color:#323232; font-size:14px; line-height:22px;}
.downapp_bbox{ background:#fff; text-align:center; padding:26px 0 30px; color:#323232; -webkit-box-shadow:0 0 10px #dcdcdc; box-shadow:0 0 10px #dcdcdc; margin:60px 10px 0px; position:relative;}
.add_ball{ position:absolute; top:-6px; left:50%; margin-left:-21px; display:block; background:url(../images/add_ball.png) no-repeat; background-size:42px 26px; width:42px; height:26px;}
.downapp_bbox h4{ font-size: 14px; margin-top: 20px;} 
.downapp_bbox h4:before,.downapp_bbox h4:after{ content: ""; display: inline-block; width: 65px; height:1px; background: #bdbdbd; vertical-align: middle; margin-right: 16px;} 
.downapp_bbox h4:after{margin-left: 16px; margin-right: 0;}
.downapp_logo{display:block; background:url(../images/downapp_logo.png) no-repeat; background-size:129px 118px; width:129px; height:118px; margin:48px auto 0; }
.downapp_btn{ display:block; margin:20px 15px 0; height:40px; line-height:40px; text-align:center; border:1px solid #ffd9e6; color:#ff4081; font-size:16px; font-weight:bold; }


/*led导航条长期活动*/
.xz5_pop{width: 255px;height: 315px;overflow: hidden;border-radius: 5px;background: url("../images/xiaozhu_h5_pop_bg1.png") 0 0 no-repeat;background-size: 100% 100%;position: absolute;left: 50%;top: 50%;
-webkit-transform: translate(-50%,-50%);-moz-transform: translate(-50%,-50%);-ms-transform: translate(-50%,-50%);-o-transform: translate(-50%,-50%);transform: translate(-50%,-50%);}
.xz5_bg1{background-image: url(../images/xiaozhu_h5_pop_bg1.png);}
.xz5_bg2{background-image: url(../images/xiaozhu_h5_pop_bg2.png);}
.xz5_btn{position: absolute;width: 195px;height: 35px;bottom: 19px;left: 50%;background: url("../images/xz5_pop_btn.png") 0 0 no-repeat;background-size: 100% 100%;
-webkit-transform: translateX(-50%);-moz-transform: translateX(-50%);-ms-transform: translateX(-50%);-o-transform: translateX(-50%);transform: translateX(-50%);overflow: hidden;}
.xz5_h5_close{position: absolute;right: 10px;top: 10px;width: 14px;height: 14px;background: url("../images/xiaozhu_h5_close_icon.png") 0 0 no-repeat;background-size: 100% 100%;}
.xz5_btn.notLogin{background-image: url(../images/xz5_pop_btn_02.png);}
/*shareV2*/
.sharev2_mask {position: fixed;top:0;left:0;z-index:1001; width: 100%;height: 100%;background: #000;filter: alpha(opacity=80);-moz-opacity: 0.8;-khtml-opacity: 0.8;opacity: 0.8;display: none;}
.sharev2_way_warp{ display: none;position:fixed; bottom:0; left:0; z-index:1002; padding:0 10px 10px 10px; width:100%; box-sizing:border-box; -webkit-box-sizing:border-box;}
.sharev2_way_box{ background:#fff; border-radius:6px; -webkit-border-radius:6px;}
.sharev2_way_box h1{ text-align:center; font-size:14px; color:#323232; padding:18px 0 15px;}
.sharev2_way_box h2{ text-align:center;}
.sharev2_pink_select,.sharev2_black_select{ display:inline-block; background:url(/images/sharev2_way_sprite1.png) no-repeat 0 0; background-size:100%; width:14px; height:14px; vertical-align:-2px; margin-right:3px;}
.sharev2_black_select{ background-position:0 -15px; background-size:100%;}
.sharev2_way_box h2 span{ color:#f05b72;}
.sharev2_way_ul{ padding-bottom:25px;}
.sharev2_way_ul li{ float:left; width:33.3%; margin-top:20px; text-align:center;}
.sharev2_qq_ico,.sharev2_wx_ico,.sharev2_pyq_ico,.sharev2_wb_ico,.sharev2_cj_ico,.sharev2_gd_ico{ display:inline-block; background:url(/images/sharev2_way_sprite2.png) no-repeat 0 0; background-size:100%; width:46px; height:47px;}
.sharev2_wx_ico{ background-position:0 -51px; background-size:100%;}
.sharev2_pyq_ico{ background-position:0 -102px; background-size:100%;}
.sharev2_wb_ico{ background-position:0 -155px; background-size:100%;}
.sharev2_cj_ico{ background-position:0 -207px; background-size:100%;}
.sharev2_gd_ico{ background-position:0 -259px; background-size:100%;}
.sharev2_way_ul li h4{ margin-top:5px;}
.sharev2_way_btn{ background:#fff; border-radius:6px; -webkit-border-radius:6px; height:45px; line-height:45px; text-align:center; color:#323232; font-size:16px; margin-top:10px;}
.app_share_img{ display: none;width:60%; position:fixed; top:13%; left:50%; transform:translateX(-50%); -webkit-transform:translateX(-50%); z-index:1002;}
.righttop_share_img{ display: none;width:60%; position:fixed; top:8%; left:50%; transform:translateX(-40%); -webkit-transform:translateX(-40%); z-index:1002;}
