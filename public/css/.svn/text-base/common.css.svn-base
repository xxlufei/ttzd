#main {position: relative;display: block;overflow: hidden;}
#toolbar {
    position: fixed;
    top: 0;
    right: -60px;
    width: 58px;
    height: 100%;
    background: #fff;
    border-left: 1px solid #f05a28;
    -moz-box-shadow:0 0 2px rgba(0,0,0,.5);
    -webkit-box-shadow:0 0 2px rgba(0,0,0,.5);
    box-shadow:0 0 7px #333;
    z-index: 9999;
}
#toolbar ul.btns {
    display: block;
}
#toolbar ul.btns li {
    display: block;
}
#toolbar ul.btns li a.btn {
    display: block;
    width: 58px;
    height: 57px;
    color: #5f6f80;
    text-align: center;
    padding-top: 11px;
}
#toolbar ul.btns li a.selected {
    color: #fff;
    background: #95a5a6;
}
#toolbar ul.btns li a.btn img.avatar {
    display: block;
    width: 32px;
    height: 32px;
    border: 2px solid #fff;
    margin: auto;
    -moz-border-radius:5px;
    -webkit-border-radius:5px;
    border-radius:5px;
}
#toolbar ul.btns li a.btn img.icon {
    display: block;
    margin: auto;
}
#toolbar ul.btns li a.btn span.title {
    font-size: 9pt;
    line-height: 200%;
    display: block;
    width: 56px;
    height: 20px;
    overflow: hidden;
    text-align: center;
}
#toolbar a.btn_tail {
    position: absolute;
    bottom: 50px;
    left: -50px;
}
#fullmask {
    display: none;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #000;
    filter: alpha(opacity=70);
    -moz-opacity: 0.7;
    -khtml-opacity: 0.7;
    opacity: 0.7;
    z-index: 9998;
}
.fullmask {
    position: absolute;
    top:0;
    left:0;
    width: 100%;
    height: 100%;
    background: #000;
    filter: alpha(opacity=50);
    -moz-opacity: 0.5;
    -khtml-opacity: 0.5;
    opacity: 0.5;
    display: none;
}
.floatlayer {
    position: absolute;
    background: #e2e2e2;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 1000;
    display: none;
    overflow: auto;
}
.floatlayer .floatwrap {
    padding: 12px;
}
.floatlayer .floatwrap h2 {
    display: block;
    background: #f4f4f4;
    color: #85929e;
    font-size: 14px;
    height: 40px;
    line-height: 38px;
    text-align: center;
}
.floatlayer .floatwrap ul {
    display: block;
    overflow: hidden;
    background: #fff;
}
.floatlayer .floatwrap ul li {
    display: inline-block;
    height: 40px;
    float: left;
    padding: 2px;
}
.floatlayer .floatwrap ul li a {
    display: block;
    font-size: 11pt;
    line-height: 40px;
    padding: 0 16px;
    background: #ced6df;
    color: #7188a0;
    text-align: center;
}
.floatlayer .floatwrap ul li.line {
    display: block;
    width: 100%;
    height: 40px;
    padding: 0;
}
.floatlayer .floatwrap ul li.line a {
    display: block;
    font-size: 11pt;
    line-height: 40px;
    padding: 0 20px;
    color: #5f6f80;
    background: #fff;
    text-align: center;
}
.floatlayer .floatwrap ul li a.selected {
    background: #3498db;
    color: #fff;
}
.centerwrap {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: auto;
    z-index:1000;
}
.scrollable {
    -webkit-overflow-scrolling: touch;
    overflow-y:auto;
}

